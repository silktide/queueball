{
  "parameters": {
    "awsKey": "",
    "awsSecret": "",
    "sqsRegion": "",
  },

  "services": {
    "sqsClient": {
      "class": "Aws\\Sqs\\SqsClient",
      "factoryClass": "Aws\\Sqs\\SqsClient",
      "factoryMethod": "factory",
      "arguments": [
        {
          "key": "%awsKey%",
          "secret": "%awsSecret%",
          "region": "%sqsRegion%"
        }
      ]
    },

    "messageFactory": {
      "class": "Silktide\\QueueBall\\Message\\SqsMessageFactory"
    },

    "queue": {
      "class": "Silktide\\QueueBall\\Queue\\SqsQueue",
      "arguments": [
        "@sqsClient",
        "@messageFactory"
      ]
    }
  }
}