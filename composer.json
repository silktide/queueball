{
  "name": "silktide/queueball",
  "description": "Silktide QueueBall, a message queue abstraction module",
  "license": "proprietary",
  "homepage": "https://github.com/silktide/queueball",
  "require": {
    "php": ">=5.3.0",
    "guzzle/guzzle": "3.*",
    "aws/aws-sdk-php": "3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.9.*"
  },
  "autoload": {
    "psr-4": {
      "Silktide\\QueueBall\\": "src"
    }
  },
  "extra": {
    "downsider-puzzle-di": {
      "silktide/syringe": {
        "path": "config/syringe.json"
      }
    }
  }
}